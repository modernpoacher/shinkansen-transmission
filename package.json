{
  "name": "shinkansen-transmission",
  "version": "2.1.7",
  "description": "Shinkansen Transmission",
  "main": "./index.js",
  "keywords": [
    "Shinkansen",
    "Transmission",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-transmission.git"
  },
  "author": "Jonathan Perry for Modern Poacher Limited",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run lint:fix && npm run babel",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint src test *.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test mocha test --recursive -r @babel/register -r @babel/polyfill --verbose",
    "coverage": "nyc --reporter=lcov npm run test",
    "pre-commit": "npm run lint && npm run coverage && npm run babel && git add lib"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
