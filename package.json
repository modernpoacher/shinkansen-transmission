{
  "name": "shinkansen-transmission",
  "version": "2.2.2",
  "description": "Shinkansen Transmission",
  "keywords": [
    "Shinkansen",
    "Transmission",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-transmission.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "cross-env DEBUG=shinkansen-transmission npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test mocha -r @babel/register test --recursive --verbose",
    "coverage": "nyc --reporter=lcov npm run test",
    "pre-commit": "npm run lint && npm run coverage && npm run build && git add lib"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.4",
    "core-js": "^3.18.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2"
  }
}
